{
    "env": {
        "browser": true,
        "es6": true,
         "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "react-app","prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        }
    },
    "plugins": [
        "react"
    ]  
}